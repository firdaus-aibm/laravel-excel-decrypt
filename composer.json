{
    "name": "firdaus-aibm/laravel-excel-decrypt",
    "description": "A Laravel package for decrypting password-protected Excel files",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Firdaus Aibm",
            "email": "info@firdaus-aibm.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "FirdausAibm\\LaravelExcelDecrypt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FirdausAibm\\LaravelExcelDecrypt\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FirdausAibm\\LaravelExcelDecrypt\\LaravelExcelDecryptServiceProvider"
            ],
            "aliases": {
                "ExcelDecrypt": "FirdausAibm\\LaravelExcelDecrypt\\Facades\\ExcelDecrypt"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}